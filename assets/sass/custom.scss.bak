.collapsible-body {
  overflow: hidden;
  display: none;
  background-color: #f4f8fd;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.collapsible-body.open {
  padding: 6px 20px !important;
}

.collapsible-body.open:hover {
  display: block !important;
  background-color: white;
}

.collapsible-body p {
  margin: 0;
  padding: 2rem;
}

.collapsible.popout {
  box-shadow: none;
}

.collapsible.popout > li {
  box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.16), 0 2px 10px 0 rgba(0, 0, 0, 0.12);
  transform: scaleX(0.92) translate3d(0, 0, 0);
  transition: margin 0.35s cubic-bezier(0.25, 0.46, 0.45, 0.94), transform 0.35s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  cursor: pointer !important;
}

.collapsible.popout > li:hover {
  will-change: margin, transform;
  color: #c7d6e0;
}

.collapsible.popout > li.active {
  box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18), 0 4px 15px 0 rgba(0, 0, 0, 0.15);
  margin: 10px 0;
  transform: scaleX(0.94) translate3d(0, 0, 0);
}

.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;
  /* Preferred icon size */
  display: inline-block;
  color: rgb(255, 171, 64) !important;
  line-height: 1;
  text-transform: none;
  letter-spacing: normal;
  word-wrap: normal;
  white-space: nowrap;
  direction: ltr;
  /* Support for all WebKit browsers. */
  -webkit-font-smoothing: antialiased;
  /* Support for Safari and Chrome. */
  text-rendering: optimizeLegibility;
  /* Support for Firefox. */
  -moz-osx-font-smoothing: grayscale;
  /* Support for IE. */
  font-feature-settings: 'liga';
}

/* Link style
/* ------------------------------ */
a {
  color: rgb(255, 171, 64);
  text-decoration: underline;
  font-weight: 600 !important;
}

a,
a > * {
  outline: none;
  cursor: pointer;
}

a:focus,
a:hover {
  color: #0b7f90;
  outline: none;
}

/* Transition elements
/* ------------------------------ */
.form-control,
.navbar a,
a {
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}

